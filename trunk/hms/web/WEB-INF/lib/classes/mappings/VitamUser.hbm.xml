<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">



<hibernate-mapping>
	<class name="com.vitam.domain.sub.impl.VitamUserImpl" table="vitamuser">
		<id column="uid" name="uid" type="java.lang.Long">
			<generator class="native" />
		</id>

		<property column="guid" name="guid" type="java.lang.String" />
		<property column="userName" name="username" type="java.lang.String" />
		<property column="password" name="password" type="java.lang.String" />
		<property column="userRole" name="userRole" type="java.lang.String" />
		<property column="question" name="question" type="java.lang.String" />
		<property column="answer" name="answer" type="java.lang.String" />
		<property column="createdOn" name="createdOn" type="java.util.Date" />
		<property column="createdBy" name="createdBy" type="java.lang.String" />
		<property column="modifiedOn" name="modifiedOn" type="java.util.Date" />
		<property column="modifiedBy" name="modifiedBy" type="java.lang.String" />
		<property column="active" name="active" type="java.lang.Long" />

	</class>
</hibernate-mapping>